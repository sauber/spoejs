% if ( -f "${doc_root}/$url_noparam" ) {
<& "/htm/$url_noparam", params => \%P, %ARGS &>
% } else {
%  $spoejs{error}{not_found} = "File doesn't exist!";
% }
<%init>

if ( 1 ) {
  use Spoejs::Text;
  use Spoejs::ChannelConf;
  use Spoejs::ChannelList;
