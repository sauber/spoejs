}

  my $doc_root =  "$ENV{DOCUMENT_ROOT}htm";
  my ($req_uri, $path) = $m->comp( '/htm/session/setupSession',  
                                   req_uri => $ENV{REQUEST_URI} );

  my ( $url_noparam, %P ) = $m->comp( 'htm/session/parseParams',
                                       url => $req_uri );
</%init>
