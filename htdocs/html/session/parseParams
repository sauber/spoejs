<%perl>
  my ( $link, $options ) = $m->comp('/html/url:decode', $url );

  # Default to configured, then 'default' theme if none is given in URL
  $$options{t} ||= #$channelconf->get(theme) || 
            'default';

  # Set theme in %spoejs
  $spoejs{theme} = $$options{t};

  return ( $link, %$options );
</%perl>
<%args>
$url => undef
</%args>
