% use Date::Manip;
<h2>**stories**</h2>
%#**storytitlelist**
% foreach my $story ( @stories ) {
% my $path = $story->story_path_from_full();
<li><a href="<& /htm/url:encode, 'story.html', d=>$path &>"><% $story->get('title') %></a> (<% $story->get('date') %>)</li>
% }
<& /w, w => 'navi', %navi_opt &>
<%args>
 @stories  => undef
 %navi_opt => undef
</%args>
