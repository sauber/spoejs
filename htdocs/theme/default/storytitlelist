<table width="300">
<tr>
  <th>Title</th><th>Date</th>
</tr>
% foreach my $story ( @stories ) {
<tr>
  <td><% $story->get( 'title' ) %></td>
  <td><% $story->get( 'date' ) %></td>
</tr>
% }
</table>



<%init>
  # Todo add div. parameters
  my $L  = new Spoejs::Lang( 'en' );
  my $SL = new Spoejs::StoryList( lang => $L );
  my @stories = $SL->list_stories(); 
</%init>