% foreach my $story ( @stories ) {
% my $path = $story->story_path_from_full();
<tr>
  <td><a href="<& /htm/url:encode, 'story.html', d => $path &>">
      <% $story->get( 'title' ) %></a></td>
  <td><% $story->get( 'date' ) %></td>
</tr>
% }
</table>
<%init>
  # Todo add div. parameters
