<table border="1">
% foreach my $story ( @stories ) {
<tr>
  <td><& /w, w => 'abstract', storyref => $story &></td>
</tr>
% }
</table>

<%init>
  # Todo add div. parameters
  my $SL = new Spoejs::StoryList( lang => $spoejs{lang_handle},
	                          path => $spoejs{channel_path} );
  my @stories = $SL->list_stories(); 
</%init>