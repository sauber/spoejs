% foreach my $story ( @stories ) {
<tr>
  <td><& abstract, storyref => $story &></td>
</tr>
% }
</table>

<%init>
  # Todo add div. parameters
  my $SL = new Spoejs::StoryList( lang => $session{lang_handle},
	 path=>"$ENV{DOCUMENT_ROOT}/users/$session{user}" );
  my @stories = $SL->list_stories(); 
</%init>