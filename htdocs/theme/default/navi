<center>
% if ( $prev_href ) {
<a href="<% $prev_href %>">
&lt;==
</a>
% } else {
&lt;==
% }
==
% if ( $next_href ) {
<a href="<% $next_href %>">
==&gt;
</a>
% } else {
==&gt;
% }
</center>
<%args>
  $next_href => undef
  $prev_href => undef
</%args>
